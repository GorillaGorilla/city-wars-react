{
  "name": "city-wars",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "connected-react-router": "^4.3.0",
    "history": "^4.7.2",
    "hoek": "^5.0.3",
    "js-binary": "^1.2.0",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hamburger-menu": "^1.1.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.4",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "socket.io-client": "^2.1.1",
    "tunnel-agent": "^0.6.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build -- --production",
    "build": "npm-run-all build-css build-js",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "npm-run-all build-css --parallel test-ci test:lint:js",
    "test-watch": "react-scripts test --env=jsdom",
    "test-ci": "CI=true npm run test-watch",
    "test:lint:js": "eslint --ext=js --ext=jsx src",
    "test-coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "^1.5.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "src/**/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/setupTests.js",
      "!src/tempPolyfills.js"
    ]
  }
}
